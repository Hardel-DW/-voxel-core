{
    "name": "@voxelio/core",
    "author": {
        "name": "Hardel",
        "email": "teams.voxel@gmail.com",
        "url": "https://voxel.hardel.io"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Hardel-DW/voxel-core.git"
    },
    "homepage": "https://voxel.hardel.io",
    "license": "MIT",
    "type": "module",
    "version": "0.4.4",
    "exports": {
        "./core": {
            "types": "./dist/core.d.ts",
            "import": "./dist/core.esm.js"
        },
        "./collections": {
            "types": "./dist/collections.d.ts",
            "import": "./dist/collections.esm.js"
        },
        "./converter": {
            "types": "./dist/converter.d.ts",
            "import": "./dist/converter.esm.js"
        },
        "./net": {
            "types": "./dist/net.d.ts",
            "import": "./dist/net.esm.js"
        },
        "./i18n": {
            "types": "./dist/i18n.d.ts",
            "import": "./dist/i18n.esm.js"
        }
    },
    "scripts": {
        "build": "rollup -c rollup.config.js",
        "biome:format": "biome format --write ./src",
        "biome:check": "biome check ./src",
        "biome:unsafefix": "biome check --write --unsafe  ./src",
        "check": "tsc --noEmit",
        "test": "vitest",
        "prepublishOnly": "pnpm run build",
        "version": "pnpm run build",
        "release:patch": "pnpm version patch && pnpm publish",
        "release:minor": "pnpm version minor && pnpm publish",
        "release:major": "pnpm version major && pnpm publish"
    },
    "dependencies": {
        "jszip": "^3.10.1",
        "zustand": "^5.0.3"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.3",
        "@rollup/plugin-commonjs": "^25.0.7",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "rollup": "^4.9.6",
        "rollup-plugin-tsconfig-paths": "^1.5.2",
        "tslib": "^2.6.2",
        "typescript": "^5.7.3",
        "vitest": "^3.0.5"
    },
    "publishConfig": {
        "access": "public"
    },
    "typesVersions": {
        "*": {
            "core": [
                "./dist/core.d.ts"
            ],
            "collections": [
                "./dist/collections.d.ts"
            ],
            "converter": [
                "./dist/converter.d.ts"
            ],
            "net": [
                "./dist/net.d.ts"
            ],
            "i18n": [
                "./dist/i18n.d.ts"
            ]
        }
    },
    "files": [
        "dist",
        "package.json"
    ]
}
